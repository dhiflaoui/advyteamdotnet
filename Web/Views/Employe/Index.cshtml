
@{
    ViewBag.Title = "Index";
}

<h2>EmloyesListe</h2>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Cin</th>
        <th>Nom</th>
        <th>Prenom</th>
        <th>Email</th>
        <th>Mot de passe</th>
        <th>Photo</th>
        <th>Cv</th>
        <th>Ville</th>
        <th>Tel</th>
        <th>Solde Conge</th>
        <th>Solde Absence</th>
        <th>Salaire</th>
        <th>Role</th>
        <th>Specialite</th>




    </tr>

    @foreach (var item in ViewBag.result)
    {
        <tr>
            <td>@item.id</td>
            <td>@item.cin</td>
            <td>@item.nom</td>
            <td>@item.prenom</td>
            <td>@item.adresseMail</td>
            <td>@item.motdp</td>
            <td>@item.photo</td>
            <td>@item.cv</td>
            <td>@item.ville</td>
            <td>@item.tel</td>
            <td>@item.solde_conge</td>
            <td>@item.solde_absence</td>
            <td>@item.salaire</td>
            <td>@item.role</td>
            <td>@item.specialite</td>


        </tr>
    }

</table>

